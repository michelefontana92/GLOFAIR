{
    "algo":"stateful",
    "meta":{
        "id":"Client_1",
        "log_dir":"logs",
        "log_file":"client_1_log.txt",
        "checkpoint_dir":"final_models",
        "checkpoint_file":"client_1_model.pt"
    },

    "data":{
        "root":"data/Adult",
        "dataset":"adult",
        "train":"node_31/adult_train.csv",
        "validation":"node_31/adult_val.csv",
        "sensible_groups":{
             "Group_Gender": {
                "gender": {
                     "privileged": "Male",
                     "unprivileged": "Female"
                     }
                 },
            "Group_Race": {
                "race": {
                     "privileged": "White",
                     "unprivileged": "Black"
                     }
                 }
    }
},

    "training":{
        "pre_training":{
            "epochs":10,
            "lr":1e-4,
            "batch_size": 128
        },

        "method": "p_norm",
        "p": 2,
        "weights": [[0.5,0.8,0.8
                   ]],
        "objectives": [
            ["CrossEntropyLoss","Group_Gender"],
            ["DDPRel","Group_Gender"],
            ["DDPRel","Group_Race"]
                   
        ],
        "scores": [["F1","Group_Race"],
                    ["DDP","Group_Gender"],
                    ["DDP","Group_Race"]
                    ],
        "pareto_weights": [1,-1,-1],
        "pareto_logdir": "ParetoFront",
        "num_starts": 1,
        "epochs": 3,
        "lr": 1e-4,
        "batch_size": 128,
        "verbose": false,
        "personalization_epochs":20,
        "ranking_function":"default",

        "model": "MLP1Hidden",
        "model_params": {
                "input": 103,
                "hidden1": 100,           
                "dropout": 0.2,
                "output": 2
            }
    },

    "monitor":{
        "monitor_log_dir":"monitors",
        "monitor_in_log_file":"monitor_in_client_1_log.txt",
        "monitor_out_log_file":"monitor_out_client_1_log.txt",
        "scores":["F1","DDP","DEO"],
        "monitor_in_save_file": "monitor_in_client_1.p",
        "monitor_out_save_file": "monitor_out_client_1.p"
    },


    "distiller":{
        "temperature":1,
        "alpha":1,
        "beta":2,
        "lr":1e-4,
        "epochs":20,
        "patience":10
    },

    "rl_checkpoint":"data/Adult/RL_Checkpoints/Client_1/checkpoint"
    
}