{
    "algo":"stateful",
    "meta":{
        "id":"Server",
        "log_dir":"logs",
        "log_file":"server_log.txt",
        "checkpoint_dir":"final_models",
        "checkpoint_file":"server_model.pt"
    },
    "training":{
        "method": "p_norm",
        "p": 2,
       "weights": [[0.2,0.4,0.4
                   ]],
        "objectives": [
               ["CrossEntropyLoss","Group_Gender"],
            ["DDPRel","Group_Gender"],
            ["DDPRel","Group_Race"]
            
                    
        ],
        "scores": [["F1","Group_Race"],
                    ["DDP","Group_Gender"],
                    ["DDP","Group_Race"]
                    ],
        "pareto_weights": [1,-1,-1],
        "pareto_logdir": "ParetoFront",
        "num_starts": 1,
        "model": "MLP1Hidden",
        "model_params": {
            "input": 103,
            "hidden1": 100,
            "dropout": 0.2,
            "output": 2
        },
        "epochs": 100,
        "batch_size": 128,
        "verbose": false,
        "ranking_function":"default"
    } 
}